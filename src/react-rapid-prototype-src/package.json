  "bugs": {
    "url": "https://github.com/starkraving/react-rapid-prototype/issues"
  },
  "homepage": "https://github.com/starkraving/react-rapid-prototype#readme",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
